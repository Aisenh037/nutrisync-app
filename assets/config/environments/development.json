{
  "firebase": {
    "projectId": "nutrisync-dev",
    "apiKey": "AIzaSyDemo-Development-Key",
    "authDomain": "nutrisync-dev.firebaseapp.com",
    "storageBucket": "nutrisync-dev.appspot.com",
    "messagingSenderId": "123456789",
    "appId": "1:123456789:web:development"
  },
  "features": {
    "enableAnalytics": false,
    "enableCrashlytics": false,
    "debugMode": true,
    "enableVoiceFeatures": true,
    "enablePremiumFeatures": true,
    "enableTestData": true,
    "enableMockServices": false
  },
  "api": {
    "baseUrl": "http://localhost:5001/nutrisync-dev/us-central1",
    "timeout": 30000,
    "retryAttempts": 3,
    "enableLogging": true
  },
  "app": {
    "name": "NutriSync Dev",
    "version": "1.0.0-dev",
    "buildNumber": "1",
    "environment": "development"
  },
  "ai": {
    "provider": "openai",
    "model": "gpt-3.5-turbo",
    "maxTokens": 1000,
    "temperature": 0.7
  },
  "database": {
    "enableOfflineSupport": true,
    "cacheSizeBytes": 104857600,
    "enablePersistence": true
  }
}