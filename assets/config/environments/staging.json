{
  "firebase": {
    "projectId": "nutrisync-staging",
    "apiKey": "AIzaSyDemo-Staging-Key",
    "authDomain": "nutrisync-staging.firebaseapp.com",
    "storageBucket": "nutrisync-staging.appspot.com",
    "messagingSenderId": "987654321",
    "appId": "1:987654321:web:staging"
  },
  "features": {
